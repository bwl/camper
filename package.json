{
  "name": "camper",
  "version": "0.1.0",
  "description": "Cozy Ink-powered companion app for the Forest knowledge base.",
  "bin": {
    "camper": "dist/index.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "bun run src/index.tsx",
    "build": "tsup src/index.tsx --format esm,cjs --target node18 --dts --out-dir dist --clean",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ink",
    "forest",
    "notes",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ink": "^4.4.1",
    "react": "^18.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.48",
    "@types/ws": "^8.5.11",
    "tsup": "^7.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}
